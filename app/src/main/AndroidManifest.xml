<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AnimationActivity" />
        <activity android:name=".AnimatorActivity" />
        <activity android:name=".TransitionActivity" />
        <activity android:name=".CustomAnimationActivity" />
        <activity android:name=".MultiPropertyActivity" />
        <activity android:name=".TranslateActivity" />
        <activity android:name=".Translate2Activity" />
        <activity android:name=".ImageActivity"></activity>
    </application>

</manifest>